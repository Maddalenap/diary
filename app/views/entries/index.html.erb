<h1>Travels</h1>
<div>
  <% for entry in @entries %>
      <h2><%= link_to(entry.title, entry_path(entry)) %></h2>
      <p><%= entry.date %></p>
      <p><%= entry.contents %></p>
  <% end %>
</div>
<button>
  <%= link_to("New entry", new_entry_path) %>
</button>
<button>
  <%= link_to("Back to favourites", root_path) %>
</button>
